{
    "name": "gos/fixture",
    "description": "Provide solution to process simple fixtures",
    "keywords": ["Fixtures", "Doctrine Fixtures", "Simple", "Easy", "Tool"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johann Saunier",
            "email": "johann_27@hotmail.fr"
        }
    ],
    "require":
    {
        "php": ">=5.4",
        "gos/parser": "~1.0@dev",
        "doctrine/common": "~2.4",
        "doctrine/data-fixtures": "~1.0",
        "symfony/finder": "~2.2"
    },
    "autoload": {
        "psr-4": { "Gos\\Component\\Fixture\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Gos\\Component\\Fixture\\Tests\\": "/Tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "minimum-stability": "dev"
    }
}
